
            <!--우측 콘텐츠 헤더영역 -->
            <div class="row wrapper border-bottom white-bg page-heading">
                <div class="col-lg-10">
                    <h2>관리자계정확인/수정</h2>
                    <ol class="breadcrumb">
                        <li>
                            <a href="/">Home</a> >
                        </li>
                        <li>
                            <a>관리자계정확인</a> >
                        </li>
                        <li class="active">
                            <strong>수정</strong>
                        </li>
                    </ol>
                </div>
                <div class="col-lg-2">

                </div>
            </div>

            <!--우측 콘텐츠 영역 -->
            <form action="/admin/modify/<%=admin.admin_member_id%>" id="adminForm" method="post">
                <div class="wrapper wrapper-content">
                    <div class="ibox-content m-b-sm border-bottom">
                        <!-- 회사코드,관리자 아이디,암호 -->
                        <div class="row">

                            <div class="col-sm-4">
                                <div class="form-group">
                                    <label class="control-label">소속회사</label>
                                    <select name="companyCode" id="companyCode" class="form-control">
                                        <option value="0">선택</option>
                                        <option value="1">자회사</option>
                                        <option value="2">협력사</option>
                                    </select>
                                </div>
                            </div>

                            <div class="col-sm-4">
                                <div class="form-group">
                                    <label class="control-label">관리자아이디</label>
                                    <input type="text" name="adminid" id="adminid" value="<%=admin.admin_id%>"  class="form-control">
                                </div>
                            </div>

                            <div class="col-sm-4">
                                <div class="form-group">
                                    <label class="control-label">관리자암호</label>
                                    <input type="password" name="admin_password" id="admin_password"  class="form-control">
                                </div>
                            </div>

                        </div>

                        <!-- 관리자명,메일주소,전화번호 -->
                        <div class="row">

                            <div class="col-sm-4">
                                <div class="form-group">
                                    <label class="control-label">관리자명</label>
                                    <input type="text" name="admin_name" id="admin_name" value="<%=admin.admin_name%>"  class="form-control">
                                </div>
                            </div>

                            <div class="col-sm-4">
                                <div class="form-group">
                                    <label class="control-label">메일주소</label>
                                    <input type="text" name="email" id="email" value="<%=admin.email%>"  class="form-control">
                                </div>
                            </div>

                            <div class="col-sm-4">
                                <div class="form-group">
                                    <label class="control-label">전화번호</label>
                                    <input type="text" name="telephone" id="telephone" value="<%=admin.telephone%>" class="form-control">
                                </div>
                            </div>

                        </div>

                        <!-- 사용여부,부서명 -->
                        <div class="row">

                            <div class="col-sm-6">
                                <div class="form-group">
                                    <label class="control-label">사용여부</label>
                                    <select name="usedYNCode" id="usedYNCode" class="form-control">
                                        <option value="0">선택</option>
                                        <option value="1">사용함</option>
                                        <option value="2">사용안함</option>
                                    </select>
                                </div>
                            </div>

                            <div class="col-sm-6">
                                <div class="form-group">
                                    <label class="control-label">부서명</label>
                                    <input type="text" name="dept_name" id="dept_name" value="<%=admin.dept_name%>" class="form-control">
                                </div>
                            </div>

                         </div>

                        <!-- 저장/목록이동 버튼  -->
                        <div class="text-center">
                            <button type="submit" class="btn btn-primary">저장</button>
                            <a href="/admin/list" class="btn btn-info">목록</a>
                        </div>

                    </div>
                </div>
            </form>

    <script>

        $(document).ready(function(){

        });



        $("#adminForm").submit(function(){

            return true;
        });

    </script>


